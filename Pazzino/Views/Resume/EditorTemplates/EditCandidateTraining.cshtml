
@model Pazzino.Models.Training

<form>

    <div class="form-group">
        <label class="">
            Field
        </label>
        @Html.Kendo().TextBoxFor(x => x.Field).HtmlAttributes(new { @class = "form-control" })
    </div>

    <div class="form-group">
        <label class="">
            Organization
        </label>
        @Html.Kendo().TextBoxFor(x => x.OrganizationName).HtmlAttributes(new { @class = "form-control" })
    </div>

    <div class="form-group">
        <label class="">
            Training Title
        </label>
        @Html.Kendo().TextBoxFor(x => x.TrainingTitle).HtmlAttributes(new { @class = "form-control" })
    </div>

    <div class="form-group">
        <label class="">
            Location
        </label>
        @Html.Kendo().TextBoxFor(x => x.Location).HtmlAttributes(new { @class = "form-control" })
    </div>




</form>