@model Pazzino.Models.JobDetailModel
@{
    ViewBag.Title = "Job Detail";
}
<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<div >
    @Html.Partial("JobSearchForm")
</div>
<div class="row">
    <div class="col-md-8">
        @Html.Raw(Model.JobDetailContent)
    </div>
    <div class="col-md-4">
        <div class="adv">
            <!-- job side ad -->
            <ins class="adsbygoogle"
                 style="display:inline-block;width:300px;height:600px"
                 data-ad-client="ca-pub-6686593824323685"
                 data-ad-slot="2972126130"></ins>
            <script>
                (adsbygoogle = window.adsbygoogle || []).push({});
            </script>
        </div>
        

    </div>


</div>

<div>
    <button class="btn btn-success" onclick="redirectTo('@Model.Url')">Apply</button>
</div>

